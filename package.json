{
  "name": "clinica-delux",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start-php": "php -S localhost:8000 -t public",
    "start-all": "concurrently \"npm run start-php\" \"npm run dev\" --names \"PHP,VITE\" --prefix-colors \"blue,green\"",
    "deploy": "chmod +x scripts/deploy.sh && scripts/deploy.sh",
    "deploy:force": "chmod +x scripts/deploy.sh && scripts/deploy.sh --force",
    "deploy:staging": "chmod +x scripts/deploy.sh && DEPLOY_TARGET=/var/www/html/grupodelux-staging scripts/deploy.sh",
    "deploy:dry-run": "chmod +x scripts/deploy.sh && scripts/deploy.sh --dry-run",
    "build:prod": "NODE_ENV=production vite build",
    "test:deploy": "curl -s http://localhost/grupodelux/api/health-check.php",
    "rollback": "echo 'Para rollback manual, restaura desde el Ãºltimo backup en /tmp/clinica_backup_*'"
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "chart.js": "^4.4.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "framer-motion": "^10.16.4",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.1",
    "lucide-react": "^0.285.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
